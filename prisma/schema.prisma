generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model StockDataExchanges {
  id        Int      @id @default(autoincrement())
  sembol    String
  son       String
  farkYuzde String
  fark      String
  alis      String
  satis     String
  aOrt      String
  adet      String
  hacim     String
  sure      String?
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @default(now()) @updatedAt @map("updated_at")
}

model StockDataFunds {
  id             Int      @id @default(autoincrement())
  sembol         String
  son            String
  farkYuzde      String
  haftaBasiYuzde String
  ayBasiYuzde    String
  yilBasiYuzde   String
  tedAdet        String
  toplamDeger    String
  sure           String?
  createdAt      DateTime @default(now()) @map("created_at")
  updatedAt      DateTime @default(now()) @updatedAt @map("updated_at")
}

model StockDataExchangesHistory {
  id        Int      @id @default(autoincrement())
  sembol    String
  son       String
  farkYuzde String
  fark      String
  alis      String
  satis     String
  aOrt      String
  adet      String
  hacim     String
  sure      String?
  createdAt DateTime @default(now()) @map("created_at")
}

model StockDataFundsHistory {
  id             Int      @id @default(autoincrement())
  sembol         String
  son            String
  farkYuzde      String
  haftaBasiYuzde String
  ayBasiYuzde    String
  yilBasiYuzde   String
  tedAdet        String
  toplamDeger    String
  sure           String?
  createdAt      DateTime @default(now()) @map("created_at")
}
